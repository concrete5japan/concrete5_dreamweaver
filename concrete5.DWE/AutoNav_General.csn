<?xml version="1.0" encoding="utf-8"?><snippet name="AutoNav General" description="General CSS for Auto-Nav upto 4 level" preview="code" type="block"><insertText location="beforeSelection"><![CDATA[/* Auto-Nav Block General */ul.nav {}/* First Level*/ul.nav li {}ul.nav li.nav-selected {} /* Selected */ul.nav li.nav-path-selected {} /* Path selected *//* Second Level */ul.nav ul li {}ul.nav ul li.nav-selected {} /* Selected */ul.nav ul li.nav-path-selected {} /* Path Selected *//* Third Level */ul.nav ul ul li {}ul.nav ul ul li.nav-selected {} /* Selected */ul.nav ul ul li.nav-path-selected {} /* Path Selected *//* Forth Level */ul.nav ul ul ul li {}ul.nav ul ul ul li.nav-selected {} /* Selected */ul.nav ul ul ul li.nav-path-selected {} /* Path Selected */]]></insertText><insertText location="afterSelection"><![CDATA[]]></insertText></snippet>